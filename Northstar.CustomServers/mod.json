{
	"ApiId" : "Northstar.CustomServers",
	"Name" : "Northstar.CustomServers",
	"Description" : "Various script patches to fix and reimplement functionality for custom multiplayer servers",
	"Authors" : [
		"BobTheBob"
	],
	"Contacts" : [
		"BobTheBob#1150"
	],
	"Version" : "0.1",
	"CustomScripts": [
